{
    "form_fields": [
      {
        "section": "1. Service Required",
        "fields": [
          {
            "field_name": "1.1 Applying for",
            "options": ["Fresh Passport", "Re-issue of Passport"]
          },
          {
            "field_name": "1.2 If re-issue, specify reason(s)",
            "options": [
              "Validity Expired within 3 years/ Due to Expire",
              "Exhaustion of Pages",
              "Validity Expired more than 3 years ago",
              "Lost Passport",
              "Change in Existing Personal Particulars",
              "Damaged Passport"
            ]
          },
          {
            "field_name": "1.3 If change in existing personal particulars, specify reason(s)",
            "options": [
              "Appearance",
              "Signature",
              "Given Name",
              "Surname",
              "Date of Birth",
              "Spouse Name",
              "Address",
              "Delete ECR",
              "Others, Please specify"
            ]
          },
          {
            "field_name": "1.4 Type of Application",
            "options": ["Normal", "Tatkaal"]
          },
          {
            "field_name": "1.5 Type of Passport Booklet",
            "options": ["36 Pages", "60 Pages"]
          },
           {
            "field_name": "1.6 Validity Required",
            "options": ["10 Years", "Up to age 18 (For minors between 15 and 18 years)"]
          }
        ]
      },
      {
        "section": "2. Applicant Details",
        "fields": [
          {
            "field_name": "2.1 Applicant's Given Name",
            "description": "(Given Name means First name followed by Middle name (If any)) (Initials not allowed)",
            "type": "text"
          },
          {
            "field_name": "Surname",
            "type": "text"
          },
          {
            "field_name": "2.2 Are you known by any other names (aliases)?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "2.3 Have you ever changed your name?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "2.4 Date of Birth (DD-MM-YYYY)",
            "type": "date"
          },
          {
             "field_name": "2.5 Place of Birth (Village or Town or City)",
             "type": "text"
          },
          {
             "field_name": "District (If born in India)",
             "type": "text"
          },
          {
             "field_name": "State/UT (If born in India)",
             "type": "text"
          },
          {
             "field_name": "Region/Country (If born abroad) If born before 15/08/1947 in a place now in Pakistan or Bangladesh, write \"Undivided India\"",
             "type": "text"
          },
  
          {
            "field_name": "2.6 Gender",
            "type": "radio",
            "options": ["Male", "Female", "Transgender"]
          },
          {
            "field_name": "2.7 Marital Status",
            "type": "radio",
            "options": ["Single", "Married", "Divorced", "Widow/Widower", "Separated"]
          },
          {
            "field_name": "2.8 Citizenship of India by",
            "type": "radio",
            "options": ["Birth", "Descent", "Registration/Naturalization"]
          },
          {
            "field_name": "2.9 PAN (If available)",
            "type": "text"
          },
          {
            "field_name": "2.10 Voter ID (If available)",
            "type": "text"
          },
          {
            "field_name": "2.11 Employment Type",
            "type": "radio",
            "options": [
              "Government",
              "PSU",
              "Statutory Body",
              "Retired Government Servant",
              "Self Employed",
              "Private",
              "Homemaker",
               "Not Employed",
              "Retired-Private Service",
              "Student",
              "Others (Owners, Partners & Directors of companies which are members of CII, FICCI & ASSOCHAM)"
            ]
          },
          {
            "field_name": "2.12 If employed in Government/ Statutory Body/ PSU, specify organization name",
            "type": "text"
          },
          {
            "field_name": "2.13 Is either of your parent (in case of minor)/ spouse, a government servant?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "2.14 Educational Qualification",
            "type": "radio",
            "options": ["7th pass or less", "Between 8th and 9th Standard", "10th pass and above", "Graduate and above"]
          },
          {
            "field_name": "2.15 Are you eligible for Non-ECR category?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
           {
            "field_name": "2.16 Visible Distinguishing Mark",
            "type": "text"
          },
          {
            "field_name": "2.17 Aadhaar Number",
            "type": "text"
          }
        ]
      },
      {
         "section": "3. Family Details",
         "fields": [
          {
            "field_name": "3.1 Father's Given Name",
            "description": "(Given Name means First name followed by Middle name (If any)) (Initials not allowed)",
            "type": "text"
          },
          {
            "field_name": "Surname",
            "type": "text"
          },
          {
            "field_name": "3.2 Mother's Given Name",
            "description": "(Given Name means First name followed by Middle name (If any)) (Initials not allowed)",
            "type": "text"
          },
          {
            "field_name": "Surname",
            "type": "text"
          },
          {
            "field_name": "3.3 Legal Guardian's Given Name (If applicable)",
             "description": "(Initials not allowed)",
            "type": "text"
          },
          {
            "field_name": "Surname",
            "type": "text"
          },
          {
            "field_name": "3.4 Spouse's Given Name (If applicable)",
            "description": "(Given Name means First name followed by Middle name (If any)) (Initials not allowed)",
            "type": "text"
          },
          {
            "field_name": "Surname",
            "type": "text"
          },
          {
            "field_name": "3.5 If applicant is minor, provide following details - Parent's Passport Details (If passport has been applied for but not received, give File Number)",
            "type": "text"
          },
          {
            "field_name": "Father/ Legal Guardian's File/ Passport Number",
            "type": "text"
          },
           {
            "field_name": "Father/ Legal Guardian's Nationality, if not Indian",
            "type": "text"
          },
          {
            "field_name": "Mother/ Legal Guardian's File/ Passport Number",
            "type": "text"
          },
           {
            "field_name": "Mother/ Legal Guardian's Nationality, if not Indian",
            "type": "text"
          }
        ]
      },
      {
        "section": "4. Present Residential Address Details (Where applicant presently resides)",
        "fields": [
          {
            "field_name": "4.1 House No. and Street Name",
            "type": "text"
          },
          {
            "field_name": "Village or Town or City",
            "type": "text"
          },
          {
            "field_name": "District",
            "type": "text"
          },
          {
            "field_name": "Police Station",
            "type": "text"
          },
          {
            "field_name": "State/UT",
            "type": "text"
          },
          {
            "field_name": "PIN",
            "type": "text"
          },
          {
            "field_name": "Mobile Number",
            "type": "text"
          },
          {
            "field_name": "Telephone Number",
            "type": "text"
          },
          {
            "field_name": "E-mail ID",
            "type": "email"
          },
          {
            "field_name": "4.2 Is permanent address same as present address?",
            "type": "radio",
            "options": ["Yes", "No"]
          }
        ]
      },
      {
        "section": "5. Emergency Contact Details",
        "fields": [
          {
            "field_name": "Name and Address (Mention address only if different from present residential address)",
            "type": "text"
          },
          {
            "field_name": "Mobile Number",
            "type": "text"
          },
          {
            "field_name": "Telephone Number",
            "type": "text"
          },
          {
            "field_name": "E-mail ID",
            "type": "email"
          }
        ]
      },
      {
        "section": "6. Previous Passport/ Application Details",
        "fields": [
          {
            "field_name": "6.1 Details of latest held/ existing/ lost/ damaged Ordinary Passport/ Identity Certificate - Passport/ Identity Certificate Number",
            "type": "text"
          },
          {
            "field_name": "Date of Issue (DD-MM-YYYY)",
            "type": "date"
          },
          {
            "field_name": "Date of Expiry (DD-MM-YYYY)",
            "type": "date"
          },
          {
            "field_name": "Place of Issue",
            "type": "text"
          },
          {
            "field_name": "6.2 Have you ever applied for passport, but not issued?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "File Number",
            "type": "text"
          },
          {
            "field_name": "Month and Year of applying",
            "type": "text"
          },
          {
            "field_name": "Name of passport office where applied",
            "type": "text"
          }
        ]
      },
     {
        "section": "7. Other details",
        "fields": [
          {
            "field_name": "7.1.1 Are any proceedings in respect of an offence alleged to have been committed by you pending before a criminal court in India?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.1.2 Has any warrant or summons for your appearance been issued & pending before a court under any law for the time being in force?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.1.3 Has a warrant for your arrest been issued by a court under any law for the time being in force?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.1.4 Has an order prohibiting your departure from India been made by any court?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.2.1 Have you, at any time during the period of five years immediately preceding the date of this application, been convicted by a court in India for any offence involving moral turpitude and sentenced in respect thereof to imprisonment for not less than two years?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.3.1 Have you ever been refused/denied passport?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.3.2 Has your passport ever been impounded?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.3.3 Has your passport ever been revoked?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.4.1 Have you ever been granted citizenship by any other region/country?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.4.2 Have you ever held the passport of any other region/country at any time?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.4.3 Have you ever surrendered your Indian passport?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
            "field_name": "7.4.4 Have you ever applied for renunciation of Indian citizenship?",
            "type": "radio",
            "options": ["Yes", "No"]
          },
          {
             "field_name": "7.5.1 Have you ever returned to India on Emergency Certificate (EC)?",
             "type": "radio",
             "options": ["Yes", "No"]
          },
          {
             "field_name": "7.5.2 Have you ever been deported from any country?",
             "type": "radio",
             "options": ["Yes", "No"]
          },
          {
             "field_name": "If yes, was the cost borne by the Government of India and if so, has the same been refunded?",
             "type": "text"
          },
          {
             "field_name": "7.5.3 Have you ever been repatriated from any country back to India?",
             "type": "radio",
             "options": ["Yes", "No"]
          },
          {
             "field_name": "If yes, was the cost borne by the Government of India and if so, has the same been refunded?",
             "type": "text"
          }
        ]
      },
      {
        "section": "8. Fee Details (Not to be filled by applicants submitting the application at Passport Seva Kendra)",
        "fields": [
          {
            "field_name": "8.1 Fee amount in (Rs)",
            "type": "text"
          },
          {
            "field_name": "8.2 If paid by Demand Draft (DD), provide the following details - DD Number",
            "type": "text"
          },
          {
            "field_name": "DD Issue Date",
            "type": "date"
          },
          {
            "field_name": "Bank Name",
            "type": "text"
          },
          {
            "field_name": "Branch",
            "type": "text"
          },
          {
            "field_name": "DD Expiry Date",
            "type": "date"
          }
        ]
      },
     {
