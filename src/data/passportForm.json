         }
       ]
     },
     {
       "section": "10. Declaration",
       "fields": [
         {
           "field_name": "I hereby declare that the information provided in this application is true and correct to the best of my knowledge and belief. I understand that any false information or misrepresentation may lead to rejection of my application or cancellation of my passport.",
           "type": "checkbox"
         },
         {
           "field_name": "Signature/Thumb Impression of Applicant",
           "type": "text"
         },
         {
           "field_name": "Date",
           "type": "date"
         }
       ]
     }
    ]
